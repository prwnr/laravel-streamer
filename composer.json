{
    "name": "prwnr/laravel-streamer",
    "description": "Events streaming package for Laravel that uses Redis 5 streams",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Rafal Purwin",
            "email": "purwinr@gmail.com"
        }
    ],
    "require": {
      "ext-json": "*",
      "ext-redis": "*",
      "php": "^8.1",
      "illuminate/console": "^8.0|^9.0",
      "illuminate/support": "^8.0|^9.0",
      "illuminate/redis": "^8.0|^9.0",
      "illuminate/container": "^8.0|^9.0"
    },
    "require-dev": {
      "orchestra/testbench": "^6.0|^7.0",
      "phpunit/phpunit": "^8.0|^9.0",
      "predis/predis": "^1.1",
      "rector/rector": "^0.14.2",
        "laravel/pint": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Prwnr\\Streamer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "aliases": {
                "Streamer": "Prwnr\\Streamer\\Facades\\Streamer"
            },
            "providers": [
                "Prwnr\\Streamer\\StreamerProvider"
            ]
        }
    }
}
