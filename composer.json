{
  "name": "prwnr/laravel-streamer",
  "description": "Events streaming package for Laravel that uses Redis 5 streams",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Rafal Purwin",
      "email": "purwinr@gmail.com"
    }
  ],
  "require": {
    "ext-json": "*",
    "ext-redis": "*",
    "php": "^7.2",
    "illuminate/console": "^6.0|^7.0",
    "illuminate/support": "^6.0|^7.0",
    "illuminate/redis": "^6.0|^7.0",
    "illuminate/container": "^6.0|^7.0"
  },
  "require-dev": {
    "orchestra/testbench": "^4.0|^5.0",
    "phpunit/phpunit": "^8.0|^9.0",
    "predis/predis": "^1.1"
  },
  "autoload": {
    "psr-4": {
      "Prwnr\\Streamer\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "laravel": {
      "aliases": {
        "Streamer": "Prwnr\\Streamer\\Facades\\Streamer"
      },
      "providers": [
        "Prwnr\\Streamer\\StreamerProvider"
      ]
    }
  }
}
